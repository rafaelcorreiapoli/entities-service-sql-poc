{
  "name": "entities-service-sql",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node",
      "graphql"
    ]
  },
  "scripts": {
    "start": "dotenv -- nodemon -e ts,graphql -x ts-node src/index.ts --ignore __tests__/integration.ts",
    "dev": "npm-run-all --parallel start",
    "debug": "dotenv -- nodemon -e ts,graphql -x ts-node --inspect src/index.ts",
    "build": "rimraf dist && tsc",
    "test": "jest --watchAll"
  },
  "dependencies": {
    "graphql-relay": "^0.5.4",
    "graphql-sequelize": "^5.6.1",
    "graphql-yoga": "^1.2.3",
    "pg": "^7.4.1",
    "pg-hstore": "^2.3.2",
    "sequelize": "^4.33.0"
  },
  "devDependencies": {
    "@types/jest": "^22.1.1",
    "dotenv-cli": "^1.4.0",
    "graphql-cli": "^2.13.1",
    "jest": "^22.2.1",
    "nodemon": "^1.14.12",
    "npm-run-all": "^4.1.2",
    "rimraf": "^2.6.2",
    "supertest": "^3.0.0",
    "ts-jest": "^22.0.3",
    "ts-node": "^4.1.0",
    "typescript": "^2.7.1"
  }
}
